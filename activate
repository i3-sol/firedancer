#!/bin/sh

if [ $# -gt 0 ] && [ "$1" = "install" ]; then
  # Install Packages needed for dev hosts
  sudo yum install                        \
    jump_module_gcc-9.3.0                 \
    jump_module_asic-sw-components-0.11.7 \
    jump_module_jmp-1.17.0
fi

module purge
module load gcc-9.3.0
module list

